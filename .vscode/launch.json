    "version": "0.2.0",
    "configurations": [
        {
            "name": "reflex",
            "type": "python",
            "request": "launch",
            "module": "reflex",
            "cwd": "${workspaceFolder}",
            "console": "integratedTerminal",
            "justMyCode": true,
            "args": [
                "run",
                "--env=dev",
                "--loglevel=info",
            ],
            "env": {
                "ENV_FOR_DEMO": "dev",
                "PYDEVD_DISABLE_FILE_VALIDATION": "1"
            }
        }
    ]
}